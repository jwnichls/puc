<?xml version="1.0" encoding="utf-8"?>
<spec name="AxisCamera" version="PUC/2.1">
  <labels>
    <label>Axis UPnP Camera</label>
  </labels>
  <groupings>
    <group name="Controls">
      <group name="Sliders">
        <labels>
          <label>Controls</label>
        </labels>
        <state name="Pan">
          <type>
            <integer>
              <min>
                <static value="-100" />
              </min>
              <max>
                <static value="100" />
              </max>
            </integer>
          </type>
          <labels>
            <label>Pan</label>
          </labels>
        </state>
        <state name="Tilt">
          <type>
            <integer>
              <min>
                <static value="-25" />
              </min>
              <max>
                <static value="25" />
              </max>
            </integer>
          </type>
          <labels>
            <label>Tilt</label>
          </labels>
        </state>
        <state name="Zoom">
          <type>
            <integer>
              <min>
                <static value="1" />
              </min>
              <max>
                <static value="9999" />
              </max>
            </integer>
          </type>
          <labels>
            <label>Zoom</label>
          </labels>
        </state>
      </group>
      <command name="Home">
        <labels>
          <label>Home</label>
        </labels>
      </command>
    </group>
    <group name="Presets">
      <state name="Presets">
        <type name="PresetType">
          <enumerated>
            <item-count>10</item-count>
          </enumerated>
          <value-labels>
            <map index="1">
              <labels>
                <refstring state="PresetName1" />
              </labels>
              <active-if>
                <equals state="Preset1Avail">
                  <static value="True" />
                </equals>
              </active-if>
            </map>
            <map index="2">
              <labels>
                <refstring state="PresetName2" />
              </labels>
              <active-if>
                <equals state="Preset2Avail">
                  <static value="True" />
                </equals>
              </active-if>
            </map>
            <map index="3">
              <labels>
                <refstring state="PresetName3" />
              </labels>
              <active-if>
                <equals state="Preset3Avail">
                  <static value="True" />
                </equals>
              </active-if>
            </map>
            <map index="4">
              <labels>
                <refstring state="PresetName4" />
              </labels>
              <active-if>
                <equals state="Preset4Avail">
                  <static value="True" />
                </equals>
              </active-if>
            </map>
            <map index="5">

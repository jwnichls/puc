									</map>
									<map index="7">
										<labels>
											<label>Hawaii</label>
											<label>GMT-10</label>
										</labels>
									</map>
								</value-labels>
							</type>
							<labels>
								<label>Time Zone</label>
							</labels>
						</state>
						<state name="DaylightSavings">
							<apply-type name="YesNoType" />
							<labels>

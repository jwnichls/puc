									</max>
								</integer>
							</type>
							<labels>
								<label>Year</label>
							</labels>
						</state>
						<state name="Date">
							<type>
								<integer>
									<min>
										<static value="1" />
									</min>
									<max>
										<static value="31" />
									</max>
								</integer>
								<value-labels>
									<!-- I could bother with coding leap years in here, 
										but I won't. Instead I'm choosing to leave that
										up to the appliance. -->
									<map index="30">
										<labels>
											<label>30</label>
										</labels>
										<active-if>
											<not>
												<equals state="Month">
													<static value="2" />
												</equals>
											</not>
										</active-if>
									</map>
									<map index="31">
										<labels>
											<label>31</label>
										</labels>
										<active-if>
											<equals state="Month">
												<static value="1" />
											</equals>
											<equals state="Month">
												<static value="3" />
											</equals>
											<equals state="Month">
												<static value="5" />
											</equals>
											<equals state="Month">
												<static value="7" />
											</equals>
											<equals state="Month">
												<static value="8" />
											</equals>
											<equals state="Month">
												<static value="10" />
											</equals>
											<equals state="Month">
												<static value="12" />
											</equals>
										</active-if>
									</map>
								</value-labels>
							</type>
